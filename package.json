{
  "name": "@astrazeneca/secure-s3-construct",
  "version": "1.0.0",
  "description": "Enterprise-grade CDK construct for secure S3 bucket provisioning",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy:dev": "cdk deploy --all --require-approval never --context env=dev",
    "deploy:prod": "cdk deploy --all --require-approval never --context env=prod",
    "diff": "cdk diff",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "security:audit": "npm audit --audit-level=moderate",
    "pre-commit": "npm run lint && npm run format:check && npm run test && npm run security:audit"
  },
  "keywords": [
    "cdk",
    "aws",
    "s3",
    "security",
    "enterprise",
    "astrazeneca"
  ],
  "author": "AstraZeneca DevOps Team",
  "license": "MIT",
  "dependencies": {
    "aws-cdk-lib": "^2.110.1",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "aws-cdk": "^2.110.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-html-reporters": "^3.1.7",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "source-map-support": "^0.5.21"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
